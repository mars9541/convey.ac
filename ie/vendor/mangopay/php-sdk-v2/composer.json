{
  "name": "mangopay/php-sdk-v2",
  "description": "PHP SDK for MANGOPAY",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-openssl": "*",
    "psr/log": "^1.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "MangoPay\\": "MangoPay/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MangoPay\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.36 || ^5.7.27 || 7.3"
  }
}
